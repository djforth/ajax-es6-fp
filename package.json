{
  "name": "@djforth/ajax-es6-fp",
  "version": "1.1.6",
  "description": "Promised Based Ajax Helper - With RoR in mind",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "morsedigital",
    "globals": {
      "jasmine": true,
      "fdescribe": true,
      "describe": false,
      "beforeEach": false,
      "afterEach": false,
      "it": true,
      "expect": true,
      "spyOn": true
    },
    "rules": {
      "react/jsx-uses-vars": 1,
      "require-jsdoc": 0
    }
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma": {
      "input": "./spec"
    }
  },
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/ajax-es6-fp.git"
  },
  "keywords": [
    "AJAX",
    "promise",
    "RubyOnRails"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/ajax-es6-fp/issues"
  },
  "homepage": "https://github.com/djforth/ajax-es6-fp#readme",
  "devDependencies": {
    "@djforth/ap_karma": "~1.2.6",
    "@djforth/ap_utils": "^2.1.1",
    "@djforth/morse-jasmine": "^1.0.17",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-config-morsedigital": "^1.0.4",
    "eslint-plugin-jasmine": "^1.8.1",
    "jasmine-ajax": "^3.2.0",
    "lodash": "^4.13.1",
    "sinon": "^1.17.4"
  }
}
