{
  "plugins": [
    [
      "lodash-modularize",
      {
          "lodashVersion": "4.17.4"
      }
    ]
  ],
  "presets": [
    "es2015",
    "es2016",
    "es2017"
  ],
  "env": {
    "karma": {
      "plugins": [
        "add-module-exports",
        [
          "istanbul",
          {
            "exclude": [
              "**/*_spec.js"
            ]
          }
        ],
        "rewire"
      ]
    }
  }
}